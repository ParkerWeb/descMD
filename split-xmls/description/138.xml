                        <rubric locusNew="1r">Anno gracie <date>75</date></rubric>
                        <incipit locusNew="1r"><name type="person">Rex pictorum rodricus</name> de <name type="place"
                                >sithia</name> ueniens in aquilonalem <name type="place">britannie</name>
                            partem</incipit>

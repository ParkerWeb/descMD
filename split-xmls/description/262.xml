                        <incipit locusNew="1r">A(mantissimo) patri et domino <name type="person">E. abbati
                                Rieuallis</name></incipit>
                        <explicit locusNew="1r">ingressum prelibans</explicit>
                        <rubric locusNew="1r">Explicit prologus</rubric>
                        <rubric locusNew="1r">Incipit liber primus. Proemium sequentis historie</rubric>
                        <incipit locusNew="1r">Historiam gentis nostre id est anglorum</incipit>
                        <ab locus="22r" locusNew="21v">Liber II</ab>
                        <ab locus="47r"><supplied>Liber</supplied> III (with capitula)</ab>
                        <ab locus="68r"><supplied>Liber</supplied> IV (capitula)</ab>
                        <ab locus="99r"><supplied>Liber</supplied> V (no capitula)</ab>
                        <ab>

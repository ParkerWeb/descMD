                    <milestone n="III" unit="volume"/>

                    <msItem n="10" locusStart="76r" locusEnd="113r">

                        <head type="CCC"><name type="author">Richard of Saint-Victor OSA</name>, <title>In uisionem
                                    <name type="person">Ezechielis</name></title></head>

                        <head type="Nasmith">

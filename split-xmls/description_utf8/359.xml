                    <msItem n="1" locusStart="1r" locusEnd="72r">
                        <head type="CCC">
                            <name type="author">Bede the Venerable</name>, <title>Historia ecclesiastica gentis
                                Anglorum</title>
                        </head>
                        <head type="James">
                            <title><name type="author">Bedae</name> Historia, etc.</title>
                        </head>

                        <rubric locus="1r">Incipit prefatio <name type="author">bede</name> presbiteri in hystoria
                            gentis anglorum </rubric>
                        <incipit locusNew="1r">Gloriosissimo regi</incipit>
                        <explicit locusNew="1r">mandare stud(u)imus</explicit>

                        <ab locus="1v">Capitula of liber I</ab>
                        <ab>Text</ab>
                        <incipit locus="2r"><name type="place">Britannia </name>oceani</incipit>

                        <ab locus="14v">Liber II</ab>

                        <ab locus="24v"><supplied>Liber</supplied> III</ab>

                        <ab locus="38v"><supplied>Liber</supplied> IV</ab>

                        <ab locus="55r"><supplied>Liber</supplied> V</ab>

                        <ab>Ending with the list of works</ab>

                        <explicit locusNew="70r">Pie intercessionis inueniam (xvi Vale. Hucusque in libro
                            impresso)</explicit>

                        <incipit locus="70r">Nunc vero si ueram supputacionem uis inuenire cui nulla ratio potest
                            contradicendo obuiare constituto capite mundi in quinquagesimo quarto anno magni cicli
                            paschalis computa usque ad christum annos iiii. clxxxij</incipit>

                        <explicit locusNew="70v">fiunt v. ccxc. qui sunt anni ab exordio mundi usque ad
                            presens</explicit>

                        <rubric locus="70v">Incipit uita uenerabilis <name type="person">Bede</name> presbiteri</rubric>
                        <incipit locusNew="70v">Opere precium est cognoscere et celebri memoria tenendum </incipit>
                        <explicit locusNew="72r">assumptus ab eodem domino et deo suo ad gratiam permanentem per omnia
                            secula seculorum. amen</explicit>

                    </msItem>


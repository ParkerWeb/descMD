                    <milestone n="I" unit="volume"/>
                    <msItem n="1" locusStart="ivr" locusEnd="193r">
                        <head type="CCC"><name type="author">Innocent III (Lotharius de Segnis) (attrib.)</name>,
                                <title>Sermones</title></head>
                        <ab>On f. <locus locusNew="ivr">1r</locus> a table of the Sermons headed</ab>
                        <incipit locus="1r" locusNew="ivr">Iste sunt sermones que continentur in presenti libro qui
                            intitulatur et attribuitur <name type="person">Innocencio pape tercio</name></incipit>
                        <ab>Text</ab>
                        <incipit locus="2r" locusNew="1r">Ecce ego mitto angelum meum, etc. Rex regum et dominus
                            dominancium pro salute populi sui</incipit>
                        <ab>The last are de <name type="person">S. Johanne ewangelista</name> and <q>de tribus regibus
                                (apertis thezauris suis)</q></ab>
                        <ab>Ending</ab>
                        <explicit locus="193r">tanquam inutilia a uita compellit</explicit>
                    </msItem>

                    <milestone n="II" unit="volume"/>
                    <msItem n="2" locusStart="194r" locusEnd="253v">
                        <head type="CCC"><name type="author">Innocent III (Lotharius de Segnis) (attrib.)</name>,
                                <title>Sermones</title></head>
                        <head type="James">Sermones</head>
                        <incipit locusNew="194r">Ecce veniet propheta magnus et ipse renouabit <name type="place"
                                >iherusalem</name> propheta magnus intelligitur christus</incipit>
                        <ab locusNew="194r">A note at the top to the effect that the leaves in this portion are
                            misplaced</ab>
                        <ab>The last is on</ab>
                        <incipit locusNew="248r">Qui habet sponsam sponsus est</incipit>
                        <ab locus="251v">Ending</ab>
                        <rubric locus="251v">Expliciunt sermones <name type="author">domini Innocencii pape
                                tercii</name></rubric>
                        <rubric locus="251v">Dignare me laudare te virgo sacrata.<lb/> Da mihi virtutem contra hostes
                            tuos, etc.</rubric>
                        <ab>ff. <locus>252r</locus>-<locus>253v</locus> blank</ab>
                    </msItem>

                    <milestone n="III" unit="volume"/>
                    <msItem n="3" locusStart="254r" locusEnd="294v">
                        <head type="CCC"><name type="author">Innocent III (Lotharius de Segnis) (attrib.)</name>,
                                <title>Sermones</title></head>
                        <ab locus="254r">Sermons in several hands</ab>
                        <incipit locusNew="254r">Cum venit plenitudo temporis, etc. In propositis verbis apostoli
                            quatuor precipue considerare debemus</incipit>
                        <ab locusNew="293r">The last is on Caro mea vere est cibus</ab>
                    </msItem>




